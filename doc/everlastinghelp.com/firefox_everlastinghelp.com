        <script>
            $(function() {

                var keys = [37, 38, 39, 40]; // left: 37, up: 38, right: 39, down: 40,
                // spacebar: 32, pageup: 33, pagedown: 34, end: 35, home: 36
                function preventDefault(e) {
                    e = e || window.event;
                    if (e.preventDefault)
                        e.preventDefault();
                    e.returnValue = false;
                }

                function keydown(e) {
                    for (var i = keys.length; i--;) {
                        if (e.keyCode === keys[i]) {
                            preventDefault(e);
                            return;
                        }
                    }
                }

                function wheel(e) {
                    preventDefault(e);
                }

                function disable_scroll() {
                    if (window.addEventListener) {
                        window.addEventListener('DOMMouseScroll', wheel, false);
                    }
                    window.onmousewheel = document.onmousewheel = wheel;
                    document.onkeydown = keydown;
                    $('body').addClass('no-scroll'); //for mobile
                }

                function enable_scroll() {
                    if (window.removeEventListener) {
                        window.removeEventListener('DOMMouseScroll', wheel, false);
                    }
                    window.onmousewheel = document.onmousewheel = document.onkeydown = null;
                    $('body').removeClass('no-scroll');
                }

                $('.login').click(function(event) {
                    event.preventDefault();
                    var $el = $('#form_login');
                    $el.prev().show();
                    $el.show();
                    disable_scroll();
                });
                $('#form_login .close-button').click(function(event) {
                    event.preventDefault();
                    var $el = $('#form_login');
                    $el.prev().hide();
                    $el.hide();
                    enable_scroll();
                });
                
                $('button.register').click(function(event) {
                    event.preventDefault();
                    var $el = $('#form_register');
                    $el.prev().show();
                    $el.show();
                });
                $('#form_register .close-button').click(function(event) {
                    event.preventDefault();
                    var $el = $('#form_register');
                    $el.prev().hide();
                    $el.hide();
                });                
                
                $('#restore-btn').click(function(event) {
                    event.preventDefault();
                    $('#form_login').hide();
                    $('#RestorePasswordForm_email').val($('#LoginForm_username').val());
                    $('#popup-restore-password').show();
                });
                
                $('#popup-restore-password .close-button').click(function(event) {
                    event.preventDefault();
                    var $el = $('#popup-restore-password');
                    $("#restore-form").show();
                    $el.prev().prev().hide();
                    $el.hide();
                });
                
                $('#popup-restore-password .back').click(function(event) {
                    event.preventDefault();
                    $("#form_login").show();
                    $("#popup-restore-password").hide();
                });
                
                $('#popup-restore-password-message .close-button, #popup-restore-password-message .btn-big').click(function(event) {
                    event.preventDefault();
                    var $el = $('#popup-restore-password-message');
                    $('#LoginForm_username').val($('#RestorePasswordForm_email').val());
                    $("#form_login").show();
                    $el.hide();
                });

