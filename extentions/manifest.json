{
"manifest_version": 2,

  "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
      "version": "1.0",
      "background": {
         "scripts": ["simhash_calculate.js"]
	 },

        "browser_action": {
	    "default_icon": "icon.png",
	        "default_popup": "popup.html"
		  },
		    "permissions": [
		        "activeTab",
			"tabs"
			      ],

			"content_scripts": [
			{
				"matches": [  "http://*/*","https://*/*"],
				"js": ["jquery-2.1.3.min.js", "md5.js"],
				"run_at": "document_end"
			}

	]


}
